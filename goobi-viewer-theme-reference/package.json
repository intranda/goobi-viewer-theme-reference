{
  "name": "goobi-viewer-theme-reference",
  "version": "3.4.0",
  "description": "This is a theme template for the Goobi viewer.",
  "author": "intranda GmbH",
  "repository": {
    "type": "git",
    "url": "https://gitea.intranda.com/goobi-viewer/goobi-viewer-theme-reference.git"
  },
  "license": "GPL-2.0-or-later",
  "scripts": {
    "dev": "cross-env GV_AUTOPREFIX=0 NODE_NO_WARNINGS=1 gulp dev",
    "build": "cross-env GV_AUTOPREFIX=0 NODE_NO_WARNINGS=1 gulp build",
    "sync": "cross-env GV_AUTOPREFIX=0 NODE_NO_WARNINGS=1 gulp sync-all",
    "cache": "cross-env GV_AUTOPREFIX=0 NODE_NO_WARNINGS=1 gulp cache",
    "target": "cross-env GV_AUTOPREFIX=0 NODE_NO_WARNINGS=1 gulp target",
    "prepare": "node ../scripts/pre-commit/install.js",
    "format": "prettier --write .",
    "test:hooks": "jest --passWithNoTests"
  },
  "devDependencies": {
    "ansi-colors": "^4.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^10.0.0",
    "fancy-log": "^2.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-less": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-riot": "^0.5.1",
    "gulp-sourcemaps": "^3.0.0",
    "merge-stream": "^2.0.0",
    "pixl-xml": "^1.0.0",
    "postcss-reporter": "^7.0.5",
    "through2": "^4.0.2"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "node",
    "rootDir": "..",
    "testMatch": [
      "<rootDir>/scripts/pre-commit/__tests__/**/*.test.js"
    ]
  }
}
